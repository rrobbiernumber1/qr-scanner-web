{
    "version": 2,
    "rewrites": [
        {
        "source": "/(.*)",
        "destination": "/index.html"
        }
    ],
    "headers": [
        {
        "source": "/(.*)",
        "headers": [
            {
            "key": "X-Frame-Options",
            "value": "DENY"
            }
        ]
        }
    ]
}